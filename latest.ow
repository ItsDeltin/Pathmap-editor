// Rule Element Count: 245
rule("Decompress map")
{
    event
    {
        Ongoing - Global;
    }

    // Element Count: 9, Condition Count: 2
    conditions
    {
        Global Variable(pfDecompress) == True;
        Host Player == True;
    }

    // Element Count: 235, Action Count: 54
    // Largest Action Index: 10 using 73 Elements
    actions
    {
        If(Count Of(Global Variable(Nodes_Node_UniqueID)));
            Abort;
        End;
        Modify Global Variable At Index(_extendedGlobalCollection, 3, Add, 1);
        Call Subroutine(LoadCompressionTable);
        Set Global Variable(i1, 0);
        Set Global Variable(i2, 0);
        "Decompress nodes"
        For Global Variable(i1, 0, Count Of(First Of(Global Variable(pfDecompress))), 1);
            For Global Variable(i2, 0, String Length(Value In Array(First Of(Global Variable(pfDecompress)), Global Variable(i1))), 3);
                Modify Global Variable(Nodes_Node_UniqueID, Append To Array, Count Of(Global Variable(Nodes_Node_UniqueID)));
                Modify Global Variable(Nodes_Node_Position, Append To Array, Vector(Divide(Subtract(Multiply(Index Of String Char(Player Variable(Host Player, compressionTable), Char In String(Value In Array(First Of(Global Variable(pfDecompress)), Global Variable(i1)), Global Variable(i2))), 2), 4096), 10), Divide(Subtract(Multiply(Index Of String Char(Player Variable(Host Player, compressionTable), Char In String(Value In Array(First Of(Global Variable(pfDecompress)), Global Variable(i1)), Add(Global Variable(i2), 1))), 2), 4096), 10), Divide(Subtract(Multiply(Index Of String Char(Player Variable(Host Player, compressionTable), Char In String(Value In Array(First Of(Global Variable(pfDecompress)), Global Variable(i1)), Add(Global Variable(i2), 2))), 2), 4096), 10)));
                Modify Global Variable(Nodes_Selected, Append To Array, False);
                Wait(0.016, Ignore Condition);
            End;
        End;
        "Decompress neighbors"
        Set Global Variable(decompressionState, 0);
        Set Global Variable(currentNode, 0);
        Set Global Variable(currentNeighbor, 0);
        For Global Variable(i1, 0, Count Of(Value In Array(Global Variable(pfDecompress), 1)), 1);
            For Global Variable(i2, 0, String Length(Value In Array(Value In Array(Global Variable(pfDecompress), 1), Global Variable(i1))), 1);
